//14.1 swap two number by using functiom (call by valu)
#include <stdio.h>

void main() {
  int a=4;
  int b=7;
  swap(a,b);
}
 void swap(int x,int y)
 {
     printf("before swap value of x=%d and y=%d \n",x,y);
     int temp;
     temp=x;
     x=y;
     y=temp;
     printf("after swaping value of x=%d and y=%d ",x,y);

 }
